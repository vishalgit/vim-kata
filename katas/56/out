utils.js:
function validateInput(data) {
    console.log('Validating input:', data);
    return data.length > 0;
}

logger.js:
class Logger {
    log(msg) {
        console.log('[Logger]:', msg);
    }
}

main.js:
const debug = true;
if (debug) {
    console.log('Debug mode enabled');
    console.log('Starting application...');
} 