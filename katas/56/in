utils.js:
function validateInput(data) {
    console.debug('Validating input:', data);
    return data.length > 0;
}

logger.js:
class Logger {
    log(msg) {
        console.debug('[Logger]:', msg);
    }
}

main.js:
const debug = true;
if (debug) {
    console.debug('Debug mode enabled');
    console.debug('Starting application...');
} 